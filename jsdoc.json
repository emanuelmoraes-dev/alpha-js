  
{
    "source": {
        "include": ["./src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "tutorials": "tutorials/",
        "template": "node_modules/tui-jsdoc-template"
    },
    "templates": {
        "name": "Alpha Restful",
        "footerText": "Alpha Restful develop by emanuelmoraes-dev",
        "tabNames": {
            "api": "API",
            "tutorials": "Tutorials"
        }
    }
}